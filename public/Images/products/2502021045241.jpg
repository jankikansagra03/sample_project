                };
                instance = modal.create(event.$target, options);
            }
            instance.update({
                header: title,
                width: getModalWidth()
            }).lock().show();
        });
        $(window).resize(function() {
            if (instance) {
                instance.update({
                    width: getModalWidth()
                });
            }
        });
    });


